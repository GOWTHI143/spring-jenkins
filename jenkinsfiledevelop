pipeline {
    agent {label 'MVN'}
    triggers { 
        pollSCM('* * * * *')
    }
    stages {
        stage ('vcs') {
            steps{
                git url: 'https://github.com/GOWTHI143/spring-petclinic.git',
                branch: 'develop'
            }
        }
    stage('build') {
        steps {
            sh 'mvn clean install'
            }
        }
    }

